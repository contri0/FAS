			<div class="home">
				<ul>
					<li><a href="/">HOME</a></li>
					<li>></li>
					<li><a href="/logfile">LOG FILE</a></li>
				</ul>
			</div>

		<div class="txt">
			<div class="before">
				<h1>Log File</h1>
				<div class="clock">
					<div class="hour">
						<div class="hr" id="hr"></div>
					</div>
					<div class="min">
						<div class="mn" id="mn"></div>
					</div>
					<div class="sec">
						<div class="sc" id="sc"></div>
					</div>
				</div>
			</div>
		</div>
		<div class="table">
			<table>
				<tr>
					<th><label>Sno.</label></th>
					<th><label>Location</label></th> 
					<th><label>Temperature</label></th> 
					<th><label>CarbonMonoxide</label></th> 
					<th><label>Smoke_in_ppm</label></th>
					<th><label>_data datetime</label></th>  
				</tr>
				<% var index = 1 ;%> 
				<% for(var row in sensorData){ %>
					<tr>
						<td><label><%=index%></label></td>
						<td><label><%=sensorData[row].location%></label></td> 
						<td><label><%=sensorData[row].temperature%></label></td> 
						<td><label><%=sensorData[row].CarbonMonoxide%></label></td> 
						<td><label><%=sensorData[row].smoke_in_ppm%></label></td>
						<td><label><%=sensorData[row]._date%></label></td>  
						<% index = index + 1 %> 
					</tr>
				<% } %> 
			</table>
		</div>
